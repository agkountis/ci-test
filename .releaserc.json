{
  "debug": true,
  "ci": true,
  "repositoryUrl": "https://github.com/agkountis/ci-test",
  "branches" : ["master"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    ["@semantic-release/git", {
      "assets": ["/home/runner/work/ci-test/ci-test/Cargo.toml"]
    }],
    ["@semantic-release/exec", {
      "prepareCmd": "cargo bump ${nextRelease.version} --manifest-path /home/runner/work/ci-test/ci-test/Cargo.toml > Cargo.toml"
    }],
    "@semantic-release/github"
  ]
}